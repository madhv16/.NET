name: dockercompose14480601767366820933
services:
  employeemanagement:
    build:
      context: C:\Users\WIIS\Desktop\New folder\Task7\EmployeeManagement
      dockerfile: EmployeeManagement/Dockerfile
    depends_on:
      mysql:
        condition: service_healthy
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      ConnectionStrings__DefaultConnection: server=mysql;port=3306;database=employee_dbdocker;user=root;password=root;TreatTinyAsBoolean=true;
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        published: "7101"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\WIIS\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\WIIS\AppData\Roaming/Microsoft/UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\WIIS\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\WIIS\AppData\Roaming/ASP.NET/Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  mysql:
    container_name: ems_mysql
    environment:
      MYSQL_DATABASE: employee_db
      MYSQL_ROOT_PASSWORD: root
    healthcheck:
      test:
        - CMD
        - mysqladmin
        - ping
        - -h
        - localhost
      timeout: 3s
      interval: 5s
      retries: 20
    image: mysql:8.0
    networks:
      default: null
    ports:
      - mode: ingress
        target: 3306
        published: "3307"
        protocol: tcp
    volumes:
      - type: volume
        source: mysql_data
        target: /var/lib/mysql
        volume: {}
networks:
  default:
    name: dockercompose14480601767366820933_default
volumes:
  mysql_data:
    name: dockercompose14480601767366820933_mysql_data